{
    "modules" : ["scripts", "Profiler", "Search", "Statistic", "Translate"],
    "provider" : {
        "useAutoloader" : true,
        "allowKillApp"  : true,
        "cache"         : "cache",
        "initServices"  : ["loader"],
        "serviceMap"    : {
            "database" : "PM\\Main\\Database",
            "config"   : "PM\\Main\\Config",
            "web"      : "PM\\Main\\Web\\App",
            "request"  : "PM\\Main\\Web\\Component\\Request",
            "response" : "PM\\Main\\Web\\Component\\Response",
            "router"   : "PM\\Main\\Web\\Component\\Router",
            "log"      : "PM\\Main\\Log",
            "access"   : "PM\\Main\\Access",
            "cache"    : "PM\\Main\\Cache",
            "loader"   : "PM\\Main\\ModuleLoader",
            "utils"    : "PM\\Main\\Utils"
        }
    },
    "cache" : {
        "driver"    : "PM\\Main\\Cache\\Session",
        "namespace" : "Performance"
    },
    "database" : {
        "address"  : "localhost",
        "user"     : "root",
        "password" : "net",
        "database" : "performance",
        "install"  : false
    },
    "access" : {
        "allowFrom" : [],
        "deniedFrom" : []
    },
    "log" : {
        "level" : 3,
        "file"  : "performance.log",
        "cache" : false
    },
    "translate" : {
        "lang" : "CS"
    },
    "profiler" : {
        "type"   : "file"
    },
    "tmpDir" : ""
}
